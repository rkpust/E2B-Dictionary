{
    "name": "E2B Dictionary",
    "description": "This is English to Bengali Dictionary.",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "E2B.png"
    },
    "icons": {
        "128": "E2B.png"
    },
    "permissions": ["activeTab"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}